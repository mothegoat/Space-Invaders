<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controllers.GameResult"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@Style.css" styleClass="backgroundImage">
    <Label fx:id="score" layoutX="375" layoutY="300" styleClass="labelInfo"/>
    <Button onMouseClicked="#getToMain" layoutX="375" layoutY="500" styleClass="Buttons" text="OK"/>
    <Label fx:id="result" layoutX="325" layoutY="100" styleClass="endLabel"/>

</Pane>
