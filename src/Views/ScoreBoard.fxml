<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controllers.ScoreBoard"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@Style.css" styleClass="backgroundImage">
    <Button text="back" styleClass="Buttons" layoutX="50" layoutY="50" onMouseClicked="#back"/>
    <Label text="ScoreBoard" styleClass="labelInfo" layoutY="100" layoutX="325"/>
    <TableView fx:id="tableScore" styleClass="scoreList" layoutX="200" layoutY="200">
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
</Pane>
